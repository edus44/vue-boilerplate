{
  "name": "vue-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve --open --copy",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "test:unit:dev": "vue-cli-service test:unit --watch --verbose",
    "test:e2e": "vue-cli-service test:e2e",
    "test:e2e:open": "cypress open",
    "test:e2e:run": "cypress run",
    "pretty": "prettier --write **/*.{js,json,scss,vue,yml}",
    "storybook": "yarn storybook:cli serve --open --copy",
    "storybook:build": "yarn storybook:cli build",
    "storybook:cli": "VUE_CLI_SERVICE_CONFIG_PATH=$(pwd)/tests/storybook/config/vue.config.js vue-cli-service"
  },
  "dependencies": {
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.10",
    "@storybook/addon-console": "^1.0.4",
    "@storybook/addon-knobs": "^3.4.10",
    "@storybook/addon-storysource": "^3.4.10",
    "@storybook/addon-viewport": "^3.4.10",
    "@storybook/vue": "^3.4.10",
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-e2e-cypress": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-plugin-unit-jest": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "@vue/eslint-config-prettier": "^3.0.1",
    "@vue/eslint-config-standard": "^3.0.1",
    "@vue/test-utils": "^1.0.0-beta.24",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "eslint-plugin-jest": "^21.22.0",
    "lint-staged": "^7.2.2",
    "node-sass": "^4.9.3",
    "prettier": "^1.14.2",
    "sass-loader": "^7.1.0",
    "storybook-addon-vue-info": "^0.6.1",
    "vue-cli-plugin-yaml": "^1.0.1",
    "vue-template-compiler": "^2.5.17",
    "yaml-jest": "^1.0.5"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "engines": {
    "node": "^8.11.3",
    "npm": "^5.6.0",
    "yarn": "^1.6.0"
  }
}
