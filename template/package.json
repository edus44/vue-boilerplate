{
  "name": "vue-boilerplate",
  "version": "0.0.1",
  "description": "Vue boilerplate",
  "main": "index.js",
  "scripts": {
    "dev": "poi -o",
    "build": "poi build",
    "build-report": "poi build --bundle-report",
    "lint": "eslint --ext .js,.vue src test",
    "test": "jest",
    "test-dev": "jest --watch --verbose",
    "test-coverage": "jest --coverage",
    "test-dev-cov": "run-p serve-coverage 'test-dev -- --coverage'",
    "serve-dist": "serve dist -s -o",
    "serve-coverage": "live-server coverage -q",
    "pretty": "prettier --write **/*.{js,json,scss,vue}",
    "precommit": "lint-staged",
    "commitmsg": "cd .. && validate-commit-msg -p eslint"
  },
  "lint-staged": {
    "*.{js,vue}": ["prettier --write", "eslint --fix", "git add"],
    "*.{scss,json}": ["prettier --write", "git add"]
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^4.18.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.12.2",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.2.2",
    "husky": "^0.14.3",
    "jest": "^22.3.0",
    "jest-serializer-vue": "^0.3.0",
    "jest-vue-preprocessor": "^1.3.1",
    "lint-staged": "^6.1.1",
    "live-server": "^1.2.0",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "poi": "^9.6.13",
    "poi-preset-bundle-report": "^2.0.2",
    "poi-preset-eslint": "^9.1.1",
    "poi-preset-yaml": "^1.0.1",
    "poi-preset-uglifyjs": "^1.0.1",
    "prettier": "^1.10.2",
    "sass-loader": "^6.0.6",
    "serve": "^6.4.10",
    "validate-commit": "^3.4.0",
    "vue-template-compiler": "^2.5.13",
    "vue-test-utils": "^1.0.0-beta.11",
    "yaml-jest": "^1.0.5"
  },
  "dependencies": {
    "vue": "^2.5.13",
    "vue-i18n": "^7.4.2",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  }
}
